<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">

	<FrameLayout
		android:id="@+id/time_circle_layout1_frame_layout"
		android:layout_width="@dimen/timeCircle_Length"
		android:layout_height="@dimen/timeCircle_Length"
		android:layout_gravity="center">

		<ImageView
			android:src="@drawable/circle_image"
			android:layout_width="@dimen/timeImage_Length"
			android:layout_height="@dimen/timeImage_Length"
			android:elevation="1dp"
			android:layout_gravity="center"
			/>

	</FrameLayout>

</LinearLayout>